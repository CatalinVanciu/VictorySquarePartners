<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CountryActivity">

    <TextView
        android:id="@+id/domainsText"
        android:layout_width="133dp"
        android:layout_height="34dp"
        android:layout_marginEnd="260dp"
        android:layout_marginBottom="664dp"
        android:gravity="right"
        android:text="@string/domains"
        android:textColor="@color/white"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/domainsSpinner"
        android:layout_width="200dp"
        android:layout_height="33dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="670dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/alphaTwoCodeText"
        android:layout_width="133dp"
        android:layout_height="34dp"
        android:layout_marginEnd="260dp"
        android:layout_marginBottom="560dp"
        android:gravity="right"
        android:text="@string/alpha_two_code"
        android:textColor="@color/white"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/alphaTwoCodeTextView"
        android:layout_width="200dp"
        android:layout_height="33dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="566dp"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/countryText"
        android:layout_width="133dp"
        android:layout_height="34dp"
        android:layout_marginEnd="260dp"
        android:layout_marginBottom="456dp"
        android:gravity="right"
        android:text="@string/country"
        android:textColor="@color/white"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/countryTextView"
        android:layout_width="200dp"
        android:layout_height="33dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="462dp"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/webPagesText"
        android:layout_width="133dp"
        android:layout_height="34dp"
        android:layout_marginEnd="260dp"
        android:layout_marginBottom="352dp"
        android:gravity="right"
        android:text="@string/webpages"
        android:textColor="@color/white"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/webPagesSpinner"
        android:layout_width="200dp"
        android:layout_height="33dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="358dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/nameText"
        android:layout_width="133dp"
        android:layout_height="34dp"
        android:layout_marginEnd="260dp"
        android:layout_marginBottom="248dp"
        android:gravity="right"
        android:text="@string/name"
        android:textColor="@color/white"
        android:textSize="16dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Spinner
        android:id="@+id/nameSpinner"
        android:layout_width="200dp"
        android:layout_height="33dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="254dp"
        android:gravity="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/stateProvinceText"
        android:layout_width="133dp"
        android:layout_height="34dp"
        android:layout_marginEnd="260dp"
        android:layout_marginBottom="144dp"
        android:gravity="right"
        android:text="@string/state_province"
        android:textColor="@color/white"
        android:textSize="14dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/stateProvinceTextView"
        android:layout_width="200dp"
        android:layout_height="33dp"
        android:layout_marginEnd="40dp"
        android:layout_marginBottom="150dp"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>